{
  "apiVersion": "1",
  "name": "mosm/map-osm",
  "title": "Map OSM",
  "description": "Description of Map OSM.",
  "category": "widgets",
  "keywords": ["hello", "block", "gutenberg", "osm", "map osm"],
  "textdomain": "map-osm",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "content": {
      "type": "string",
      "default": "Lorem text should be replaced with real text"
    },
    "latitude": {
      "type": "number",
      "default": 23.7644025
    },
    "longitude": {
      "type": "number",
      "default": 90.389015
    },
    "zoom": {
      "type": "number",
      "default": 17
    },
    "searchQuery": {
      "type": "string",
      "default": "Dhaka, Dhaka Metropolitan, Dhaka District, Dhaka Division, 1215, Bangladesh"
    },
    "controlPosition": {
      "type": "string",
      "default": "topleft"
    },
    "markerText": {
      "type": "string",
      "default": "Dhaka, Dhaka Metropolitan, Dhaka District, Dhaka Division, 1215, Bangladesh"
    },
    "height": {
      "type": "object",
      "default": {
        "desktop": "400px",
        "tablet": "300px",
        "mobile": "310px"
      }
    },
    "width": {
      "type": "object",
      "default": {
        "desktop": "600px",
        "tablet": "500px",
        "mobile": "250px"
      }
    },
    "markerIconColor": {
      "type": "object",
      "default": "orangered"
    },
    "border": {
      "type": "object",
      "default": {
        "width": "2px",
        "style": "solid",
        "color": "orangered"
      }
    },
    "mapLayer": {
      "type": "string",
      "default": "standard"
    },
    "mapOptions": {
      "type": "object",
      "default": {
        "showPrintButton": true,
        "printBtnPosition": "topright",
        "showImageDownload": false,
        "showPdfDownload": false
      }
    },
    "downloadImgBtn": {
      "type": "object",
      "default": {
        "text": "Download as Image"
      }
    },
    "downloadPdfBtn": {
      "type": "object",
      "default": {
        "text": "Download as PDF"
      }
    }
  },


  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:./../dist/editor.js"
}
